    <!-- contact form start -->
    <section class="contact-form">
      <div class="container">
        <div class="row">
          <form id="contact-form">
            <div class="col-md-6 col-sm-12">
              <div class="block">
                <div class="form-group">
                  <input
                    name="user_name"
                    id="contact_user_name"
                    type="text"
                    class="form-control"
                    placeholder="Your Name"
                  />
                </div>
                <div class="form-group">
                  <input
                    name="user_email"
                    id="contact_user_email"
                    type="text"
                    class="form-control"
                    placeholder="Email Address"
                  />
                </div>
                <div class="form-group">
                  <input
                    name="user_subject"
                    id="contact_subject"
                    type="text"
                    class="form-control"
                    placeholder="Subject"
                  />
                </div>
              </div>
            </div>
            <div class="col-md-6 col-sm-12">
              <div class="block">
                <div class="form-group-2">
                  <textarea
                    name="user_message"
                    id="contact_message"
                    class="form-control"
                    placeholder="Your Message"
                    style="resize: vertical; height: 105px"
                  ></textarea>
                </div>
                <button
                  class="btn btn-main"
                  type="submit"
                  style="background-color: #5cc6cf; border-radius: 0 0 5em 5em;font-weight: 900;"
                >
                  SEND MESSAGE
                </button>
              </div>
            </div>
          </form>
        </div>
        <div class="row">
          <div class="alert alert-danger error" id="error">
            Sorry, there was an error sending the message. Perhaps with our
            classes you or your child will fix it!
          </div>
          <div class="alert alert-success success" id="success">
            Thanks for your message, we will get back to you soon.
          </div>
        </div>

        <div class=" contact-box row">
          <div class="col-lg-3 col-md-6 col-sm-12">
            <div class="block">
              <h2 style="font-weight: bolder">SUBSCRIBE</h2>
              <!-- Begin Mailchimp Signup Form -->
              <div id="mc_embed_signup">
                <form
                  action="https://education.us18.list-manage.com/subscribe/post?u=190463ed60601a78f0ba365bf&amp;id=b4d939e2e8"
                  method="post"
                  id="mc-embedded-subscribe-form"
                  name="mc-embedded-subscribe-form"
                  class="validate"
                  target="_blank"
                  novalidate
                >
                  <div id="mc_embed_signup_scroll">
                    <div class="mc-field-group">
                      <input
                        type="email"
                        value=""
                        name="EMAIL"
                        class="required email form-control"
                        id="mce-EMAIL"
                        placeholder="Email Address"
                      />
                    </div>
                    <div id="mce-responses" class="clear">
                      <div
                        class="response"
                        id="mce-error-response"
                        style="display:none"
                      ></div>
                      <div
                        class="response"
                        id="mce-success-response"
                        style="display:none"
                      ></div>
                    </div>
                    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
                    <div
                      style="position: absolute; left: -5000px;"
                      aria-hidden="true"
                    >
                      <input
                        type="text"
                        name="b_190463ed60601a78f0ba365bf_b4d939e2e8"
                        tabindex="-1"
                        value=""
                      />
                    </div>
                    <div class="clear">
                      <input
                        type="submit"
                        value="SUBSCRIBE"
                        name="subscribe"
                        id="mc-embedded-subscribe"
                        class="button btn btn-main"
                        style="color: white; background-color: #5cc6cf; border-radius: 0 0 5em 5em;font-weight: 900;width: 100%;"
                      />
                    </div>
                  </div>
                </form>
              </div>

              <!--End mc_embed_signup-->
            </div>
          </div>
          <div class="col-lg-3 col-md-6 col-sm-12">
            <div class="block">
              <h2 style="font-weight: bolder;">VISIT US</h2>
              <ul class="address-block">
                <li>
                  <i class="ion-ios-location-outline"></i>1340 S De Anza Blvd,
                  Suite 201, San Jose CA 95129
                </li>
                <li>
                  <i class="ion-ios-email-outline"></i>Email:
                  <a href="mailto:info@techlabeducation.com"
                    >info@techlabeducation.com</a
                  >
                </li>
                <li>
                  <i class="ion-ios-telephone-outline"></i>SMS/Phone: (408) 898 4939
                </li>
              </ul>
              <ul class="social-icons">
                <li>
                  <a
