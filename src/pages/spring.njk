{% import "payment_mixins.njk" as p %}

<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js">
  <!--<![endif]-->
  <head>
    {% include "head_includes.njk" %} {% include "payment_css.njk" %}
    <style>
      .class-name {
        margin: 5px;
        text-align: center;
      }
      .blue-1 {
        --theme-color: #5cc6cf !important;
      }
      .blue-2 {
        --theme-color: #5cc6cf !important;
      }
      .blue-3 {
        --theme-color: #5cc6cf !important;
      }
      .class-row {
        border-radius: 8px;
        border: 2px solid #5cc6cf;
        padding: 8px 0 8px 0;
        display: flex;
        justify-content: center;
        align-items: center;
        margin: 10px 0 10px 0;
        flex-wrap: wrap;
        transition: all 0.2s ease-in-out;
        /*box-shadow: 3px 3px 0px 0px var(--theme-color);*/
        transform: translate(0,0);
      }
      .class-row:hover {
        box-shadow: 5px 5px 0px 0px var(--theme-color);
        transform: translate(-2px, -2px);
      }
      .class-row .col-md-2,
      .col-md-3,
      .col-md-4 {
        margin: 3px 0 3px 0;
        display: flex;
        align-items: center;
        justify-content: center;
        flex-wrap: wrap;
      }
      .flex-left {
        justify-content: flex-start;
      }
      .class-row p {
        margin: 0;
      }
    </style>
  </head>

  <body id="body">
    {% include "navbar.njk" %}

    <section class="page-title bg-6">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="block">
              <h1 style="font-weight: bolder;">Spring Classes</h1>
              <h1 class="btn btn-main animated fadeInUp" style="margin: 3px 12px 3px 12px;border-radius: 5em;font-weight: 900;" target="blank"><a href="/schedule.html">
              Spring Schedule</a></h1>
              <a href="https://lbtricvuobirgvexydwub-free.10to8.com" class="btn btn-main animated fadeInUp" style="margin: 3px 12px 3px 12px;font-weight: 900;border-radius: 5em" 
              target="blank">FREE CONSULT</a> 
            </div>
          </div>
        </div>
      </div>
    </section>

    {% include "payment_modal.njk" %}

    <section
      class="about section"
      style="display: flex; flex-direction: column; align-items: center"
    >
      <div class="container" style="margin: 0 20px 0 20px">
        <img src="images/tynkerer icon-01.png" style="width: 7em;/*! position: absolute; */float: right;margin-top: 1.25em;">
        <h1 id="tynkerer" style="font-weight: 900;">Tynkerers</h1>
        <h1 style="font-size: 27px;margin-top: 0em;font-weight: 900;color: #7b7b7b;">Grades 5+ | Intro to Programming | <a href="/schedule.html">Spring Schedule</a></h1>
        <div class="container blue-1" style="padding-bottom:30px; padding-left:0px;">
        
          {{ p.class_row(
            name="Intro to Python",
            info="Begin with programming",
            times="Thu 5-6.30p OR Sat 10.30-noon",
            prereqs="none",
            nextclass="Intermediate Python",
            id="python-intro",
            link="python-intro.html"
          ) }}
        </div>
    
        <img src="images/hammer icon-01.png" style="width: 7em;/*! position: absolute; */float: right;margin-top: 1.25em;">
        <h1 id="foundation" style="font-weight: 900;">Explorers</h1>
        <h1 style="font-size: 27px;margin-top: 0em;font-weight: 900;color: #7b7b7b;">Grades 6-9 | Foundations | <a href="/schedule.html">Spring Schedule</a></h1>
        <div class="container blue-2" style="padding-bottom:30px; padding-left:0px;">
          {{ p.class_row(
            name="Intermediate Python",
            info="Enhance programming skills in Python",
            times="Thu 5-6.30p OR Sat 10.30-noon",
            prereqs="Intro to Python",
            nextclass="Intro to AI/ML (Advanced Python) or Intro to Java",
            id="python-intermediate",
            link="python-intermediate.html"
          ) }}
          {{ p.class_row(
            name="Intro to Java",
            info="Begin programming in Java",
            times="Thu 5-6.30p OR Sat 10.30-noon",
            prereqs="none",
            nextclass="Intermediate Java",
            id="java-intro",
            link="java-intro.html"
          ) }}
        
        </div>
        <img src="images/rocket icon-01.png" style="width: 7em;/*! position: absolute; */float: right;margin-top: 1.25em;">
        <h1 id="principles" style="font-weight: 900;">Innovators</h1>
        <h1 style="font-size: 27px;margin-top: 0em;font-weight: 900;color: #7b7b7b;">Grades 8+ | Advanced Computer Science |<a href="/schedule.html"> Spring Schedule</a></h1>
        <div class="container blue-3" style="padding-left:0px;">
          
          {{ p.class_row(
            name="Intermediate Java",
            info="Enhance programming skills in Java",
            times="Thu 5-6.30p AND Sat 10.30-noon",
            prereqs="Intro to Java",
            nextclass="Advanced Java & Algorithms",
            id="java-intermediate",
            link="java-intermediate.html"
          ) }}
          {{ p.class_row(
            name="Advanced Java & APCS Prep",
            info="Object oriented programming and APCS Prep",
            times="Sat 10.30-noon AND Sun 1-2.30p",
            prereqs="Intermediate Java",
            nextclass="Intro to AI/ML",
            id="java-advanced",
            link="java-advanced.html"
          ) }}
        </div>
      

      <div class="container blue-1" style="padding-bottom:30px; padding-left:0px;">
          {{ p.class_coming(
            name="Intro to AI/ML (Advanced Python)",
            info="Data Science Fundamentals using Python",
            times="TBD",
            prereqs="Intermediate Python",
            nextclass="Intermediate Java"
          ) }}
        </div>
        </div>
    </section>

    {% include "footer.njk" %} {% include "jsincludes.njk" %} {% include
    "payment_js.njk" %}
  </body>
</html>
